a=int(input("Enter a no:"))
b=int(input("Enter a no:"))
c=int(input("Enter a no:"))
if(a>b):
    if(a>c):
        print("Greatest is:",a)
    else:
        print("Greatest is:",c)
elif(b>a):
    if(b>c):
        print("Greatest is:",b)
    else:
        print("Greatest is:",c)
else:
    print("Greatest is:",c)
     
